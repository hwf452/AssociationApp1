<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/Member_root"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="25.0dip"
        android:paddingRight="25.0dip"
        android:paddingTop="25.0dip"
        android:background="@drawable/card_bg2" >
	

            <TextView
                android:layout_marginTop="30dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:lineSpacingExtra="4.0dip"
                android:text="@string/login_about_info"
                android:textSize="18sp"
                android:textColor="@color/black" />

       
        
            
    </RelativeLayout>

    <ImageView
        android:id="@+id/contact_card_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="backButton"
       android:layout_gravity="right"
        android:src="@drawable/btn09_btn05_selector" />

</FrameLayout>