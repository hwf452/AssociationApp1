<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hot_bg" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        style="@style/TopBackgroundTitleBarstyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <Button
            android:id="@+id/button1"
            style="@style/TopTitleBackStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/textView1"
            style="@style/TopTitleTextViewStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/my_private_imfo_tv_more" />
    </RelativeLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/relativeLayout1"
        android:scrollbars="none" >

        <RelativeLayout
            android:id="@+id/member_info_layout"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <RelativeLayout
                android:id="@+id/member_info_layout1"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10.5dip"
                android:background="@drawable/button52" >

                <ImageView
                    android:id="@+id/head_icon"
                    android:layout_width="55.2dp"
                    android:layout_height="55.2dp"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="21dip"
                    android:layout_marginRight="15dp"
                    android:src="@drawable/portrait7_tab4" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toRightOf="@id/head_icon"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content" >

                        <TextView
                            android:id="@+id/member_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingRight="10.0dip"
                            android:text="@string/tv_tab4_myname"
                            android:textColor="#000000"
                            android:textSize="18.0sp" />
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/member_info_layout1"
                android:layout_marginTop="10.5dp"
                android:background="@drawable/btn53_1_btn_selector" >

                <TextView
                    android:id="@+id/textView_2"
                    style="@style/tab4_tv_item_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="电话" />

                <TextView
                    android:id="@+id/textView2"
                    style="@style/tab4_tv1_item_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/textView_2"
                    android:text="18680178306" />

                <ImageView
                    android:id="@+id/right_arrow_tip2"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate2FixPhone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate1"
                android:background="@drawable/btn53_2_btn_selector" >

                <TextView
                    android:id="@+id/textView_8"
                    style="@style/tab4_tv_item_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="固话" />
                
                <TextView
                    android:id="@+id/textViewFixPhone"
                    style="@style/tab4_tv1_item_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/textView_8"
                    android:text="0756-3389988" />

               

                <ImageView
                    android:id="@+id/right_arrow_tip3"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/phone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate2FixPhone"
                android:background="@drawable/btn53_2_btn_selector" >

                <TextView
                    android:id="@+id/textView_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/tab4_tv_item_style"
                    android:text="邮箱" />
                <TextView
                    android:id="@+id/editText3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/textView_3"
                    style="@style/tab4_tv1_item_style"
                    android:text="hwf452@163.com" />
                
                
                <ImageView
                    android:id="@+id/right_arrow_tip3"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:layout_toRightOf="@id/textView3"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/phone"
                android:background="@drawable/btn53_2_btn_selector" >

                <TextView
                    android:id="@+id/textView_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/tab4_tv_item_style"
                    android:text="公司" />
                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView_4"
                    style="@style/tab4_tv1_item_style"
                    android:text="珠海夏浪科技有限公司" />


                <ImageView
                    android:id="@+id/right_arrow_tip4"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate3"
                android:background="@drawable/btn53_2_btn_selector" >

                <TextView
                    android:id="@+id/textView_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/tab4_tv_item_style"
                    android:text="微博" />
                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView_5"
                    style="@style/tab4_tv1_item_style"
                    android:text="hwf452.weibo.com" />

                <ImageView
                    android:id="@+id/right_arrow_tip5"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate4"
                android:background="@drawable/btn53_2_btn_selector" >

                <TextView
                    android:id="@+id/textView_6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/tab4_tv_item_style"
                    android:text="微信" />
                <TextView
                    android:id="@+id/textView6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/textView_6"
                    style="@style/tab4_tv1_item_style"
                    android:text="zhgaxiehuei" />
                


                <ImageView
                    android:id="@+id/right_arrow_tip6"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/relate6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate5"
                android:background="@drawable/btn53_3_btn_selector" >

                <TextView
                    android:id="@+id/textView_7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    style="@style/tab4_tv_item_style"
                    android:text="地址" />
                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/textView_7"
                    style="@style/tab4_tv1_item_style"
                    android:text="珠海市香洲区人民东路珠信大厦512" />

                <ImageView
                    android:id="@+id/right_arrow_tip8"
                    style="@style/tab4_iv_right_style"
                    android:layout_width="27.3dp"
                    android:layout_height="27.3dp"
                    android:visibility="gone" />
            </RelativeLayout>

            <Button
                android:id="@+id/modify"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/relate6"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="30dp"
                android:background="@drawable/btn55_btn_selector"
                android:text="@string/tv_tab4_modify"
                android:textColor="@drawable/tv_rb2_selector"
                android:textSize="18sp" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>